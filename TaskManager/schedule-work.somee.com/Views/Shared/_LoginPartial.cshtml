<div class="row" style="width:20%;">
    <div class="col-4">
        <div class="list-group" id="list-tab" role="tablist">

            @foreach (var u in ViewBag.User)
            {
                <a class="list-group-item list-group-item-action" id="list-home-list" data-toggle="list" href="/Home/Task/@u.ID" role="tab" aria-controls="home">@u.Name</a>

            }
        </div>
    </div>
</div>